                <Canvas Background="DarkGray" HorizontalAlignment="Left" Height="135" VerticalAlignment="Top" Width="768">
                    <Grid Width="768">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="100" />
                            <RowDefinition Height="35" />
                        </Grid.RowDefinitions>
                        <TextBlock x:Name="TblCurrentYear" Text="{Binding CurrentYear, Mode=TwoWay}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="50" />
                        <Slider x:Name="SlrCurrentYear"  
                                Grid.Row="1" 
                                Maximum="32" 
                                BorderBrush="{x:Null}" 
                                Value="{Binding CurrentYearSliderValue, Mode=TwoWay}">
                        	<Slider.Style>
                        		<StaticResource ResourceKey="SliderStyle"/>
                        	</Slider.Style>
                        </Slider>
                    </Grid>
                </Canvas>
            </Canvas>
            <!--RightPanel-->
            <StackPanel Grid.Column="1">
                <!--Legend-->
                <Canvas Background="DarkGray" Height="135" Width="400" Margin="20,0,0,0">
                    <Grid>
                        
                    </Grid>
                </Canvas>
                
                <!--CountryInfo-->
                <Canvas Background="DarkGray" Height="420" Width="400" Margin="20,50,0,100">
                    <Grid Height="420" Width="400">
                        <Grid.RowDefinitions>
                            <!--    Nazwa, powod, cesarstwo, habsburg   -->
                            <RowDefinition Height="Auto" />
                            <!--    Wladca   -->
                            <RowDefinition Height="30" />
                            <!--    Religia   -->
                            <RowDefinition Height="30" />
                            <!--    Opis   -->
                            <RowDefinition Height="220" />
                        </Grid.RowDefinitions>
                        <StackPanel Grid.Row="0">
                            <TextBlock x:Name="tblCountryName" 
                                       Text="{Binding CountryName, Mode=TwoWay}" 
                                       FontSize="40" 
                                       HorizontalAlignment="Center"/>
                            <TextBlock x:Name="tblReason" 
                                       Text="{Binding Reason, Mode=TwoWay}" 
                                       FontSize="15" 
                                       HorizontalAlignment="Center" 
                                       Visibility="{Binding Reason, Converter={StaticResource CvtStringToVisibility}}"/>
                            <TextBlock x:Name="tblHREMember" 
                                       Text="Członek Świętego Cesarstwa Rzymskiego" 
                                       Visibility="{Binding IsHRE, Converter={StaticResource CvtBoolToVisibility}}"                                       
                                       FontSize="15" 
                                       HorizontalAlignment="Center" />
                            <TextBlock x:Name="tblIsHabsburgTerritory" 
                                       Text="We władaniu Habsburgów" 
                                       Visibility="{Binding IsHabsburg, Converter={StaticResource CvtBoolToVisibility}}"
                                       FontSize="15" 
                                       HorizontalAlignment="Center" />
                        </StackPanel>
                        <TextBlock 
                            Grid.Row="1" 
                            Text="Władca" 
                            FontSize="15" 
                            HorizontalAlignment="Left" 
                            VerticalAlignment="Bottom" 
                            Margin="20,0,0,0"/>
                        <TextBlock 
                            Grid.Row="1" 
                            x:Name="tblRuler" 
                            Text="{Binding RulerName, Mode=TwoWay}" 
                            FontSize="15" 
                            HorizontalAlignment="Right" 
                            VerticalAlignment="Bottom" 
                            Margin="0,0,20,0"/>
                        <TextBlock 
                            Grid.Row="2" 
                            Text="Wyznanie" 
                            FontSize="15" 
                            HorizontalAlignment="Left" 
                            VerticalAlignment="Bottom" 
                            Margin="20,0,0,0"/>
                        <TextBlock 
                            Grid.Row="2" 
                            x:Name="tblReligion" 
                            Text="{Binding Religion, Mode=TwoWay}" 
                            FontSize="15" 
                            HorizontalAlignment="Right" 
                            VerticalAlignment="Bottom" 
                            Margin="0,0,20,0"/>
                        <ScrollViewer Grid.Row="3" Margin="10,0,10,0" BorderBrush="{x:Null}" >
                            <TextBlock 
                                x:Name="tblDescription" 
                                TextWrapping="Wrap" 
                                FontSize="15" 
                                HorizontalAlignment="Right" 
                                VerticalAlignment="Bottom" 
                                Margin="0,0,20,0" 
                                Text="{Binding Description, Mode=TwoWay}"/>
                        </ScrollViewer>
                    </Grid>
                </Canvas>
            </StackPanel>
        </Grid>
    </ScrollViewer>
</UserControl>